<div class="container">
  <h1 class="h2 mb-5">Icon fonts used in <a href="https://www.lugolabs.com">Lugolabs.com</a></h1>

  <ul class="list-unstyled">
    <% @icons.each do |icon| %>
      <li class="mb-3">
        <button class="btn btn-lg btn-outline-primary mr-4">
          <i class="<%= icon %>"></i>
        </button>
        <%= icon %>
      </li>
    <% end %>
  </ul>
</div>
